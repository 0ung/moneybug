<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="accountGPT">
<insert id="insert" parameterType="accountGPTDTO" useGeneratedKeys="true" keyProperty="SEQ">
		insert into account_gpt values (#{seq},#{accountBookId},#{content},#{createAt})
	</insert>
	<select id="one" parameterType="String" resultType="accountGPTDTO">
		select * from account_gpt where ACCOUNT_BOOK_ID = #{accountBookId}
	</select>
	<select id="list" resultType="accountGPTDTO">
		select * from account_gpt
	</select>
</mapper>